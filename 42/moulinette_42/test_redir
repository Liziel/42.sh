ls > toto ; cat toto ; rm toto
ls>toto ; cat toto ; rm toto
ls > toto ; cat toto ; ls >> toto ; cat toto ; rm toto
>toto ls ; cat toto ; rm toto
>toto ls ; cat toto ; >>toto ls ; cat toto ; rm toto
ls > toto ; cat toto ; cat < toto > titi ; cat titi ; rm toto ; rm titi
ls > toto ; <toto cat >titi ; cat titi ; rm titi ; rm toto
ls >toto ; cat <toto > titi ; cat titi ; cat toto ; rm titi ; cat < toto >>titi ; cat titi ; cat toto ; rm titi ; rm toto
ls >> toto ; cat toto ; pwd >toto ; ls <toto ; cat toto ; rm toto

ls \> xd
ls \\> xd
$a  -> a="ls"
