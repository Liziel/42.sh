#Gestion des parenteses
(ls ; ls ; ls) > toto ; cat toto | wc

#Gestion des backcotes
touch ...a ; echo "echo plop" > ...a ; `ls` ; rm ...a

#Alias loop
alias nb1 nb2 ; alias nb2 "nb1 ; echo plop" ; nb1

#Gestion du globing
ls *.c > toto ; ls *[a-z] ; ls *.?

#parenteses imbriquees
